{
  "name": "rntemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native rnx-start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx}\" --no-error-on-unmatched-pattern",
    "format:write": "prettier --write \"./**/*.{js,jsx,ts,tsx}\" --no-error-on-unmatched-pattern",
    "rename": "npx react-native-rename",
    "check-deps": "rnx-dep-check",
    "fix-deps": "rnx-dep-check --write",
    "preinstall": "npx only-allow yarn",
    "prepare": "husky install"
  },
  "dependencies": {
    "i18next": "^21.8.3",
    "react": "17.0.2",
    "react-i18next": "^11.16.9",
    "react-native": "^0.68.0-0",
    "react-native-localize": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rnx-kit/babel-preset-metro-react-native": "^1.1.0",
    "@rnx-kit/cli": "^0.12.7",
    "@rnx-kit/dep-check": "^1.12.14",
    "@rnx-kit/metro-config": "^1.2.31",
    "@rnx-kit/metro-resolver-symlinks": "^0.1.19",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^7.32.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^12.4.1",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^6.4.7",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "rnx-kit": {
    "reactNativeVersion": "^0.68",
    "kitType": "app",
    "capabilities": [
      "babel-preset-react-native",
      "core",
      "core-android",
      "core-ios",
      "jest",
      "react",
      "react-test-renderer"
    ],
    "server": {
      "detectCyclicDependencies": true,
      "detectDuplicateDependencies": {
        "ignoredModules": ["react-is"],
        "throwOnError": true
      },
      "typescriptValidation": true
    }
  }
}
